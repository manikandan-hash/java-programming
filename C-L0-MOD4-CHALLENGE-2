import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
    
        long number = sc.nextLong();
        int digit = sc.nextInt();
        int count = 0;

        while (number > 0) {
            if (number % 10 == digit) {
                count++;
            }
            number /= 10;
        }

        System.out.print(count);
        
    }
}
