import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
         long n = sc.nextLong();
         long rev = 0,digit;

        while (n > 0) {
            digit = n % 10;       
            rev = rev * 10 + digit; 
            n = n / 10;            
        }

        System.out.println(rev);
    }
}
